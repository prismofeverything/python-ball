class A:
    def __init__(self, waka, wakainit=[]):
        self.waka = wakainit
        self.waka.extend(waka)
    def allo(self):
        return [waka % 44.4 for waka in self.waka]

class B(A):
    def __init__(self, waka):
        A.__init__(self, waka)

class C(B):
    def __init__(self, waka):
        A.__init__(self, waka)

    def allo(self):
        return [waka % 777.77 for waka in self.waka]
